{
  "name": "adminapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "prettier": "prettier \"./**/*.{js,flow,json,scss,css,html,md}\"",
    "prettier:check": "yarn prettier --check",
    "prettier:write": "yarn prettier --write",
    "flow:check": "flow check",
    "flow:restart": "flow stop && flow start",
    "flow:coverage": "flow-coverage-report --config flow-coverage.json",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:staged": "lint-staged",
    "lint:js": "eslint \"src/**/*.js\" \"config/**/*.js\"",
    "lint:css": "stylelint \"src/**/*.scss\""
  },
  "dependencies": {
    "invariant": "2.2.4",
    "normalize.css": "^8.0.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "typed-contracts": "2.3.0"
  },
  "devDependencies": {
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-css-modules": "2.11.0",
    "eslint-plugin-flowtype-errors": "4.2.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-sonarjs": "0.5.0",
    "eslint-plugin-unicorn": "20.0.0",
    "flow-bin": "0.125.1",
    "flow-coverage-report": "0.7.0",
    "node-sass": "4.14.1",
    "prettier": "2.0.5",
    "react-scripts": "3.4.1",
    "stylelint": "13.4.0",
    "stylelint-a11y": "1.2.3",
    "stylelint-at-rule-no-children": "0.3.1",
    "stylelint-config-css-modules": "2.2.0",
    "stylelint-config-prettier": "8.0.1",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-declaration-block-no-ignored-properties": "2.3.0",
    "stylelint-high-performance-animation": "1.5.1",
    "stylelint-media-use-custom-media": "2.0.0",
    "stylelint-scss": "3.17.2",
    "stylelint-selector-no-empty": "1.0.7",
    "stylelint-selector-tag-no-without-class": "2.0.3"
  }
}
